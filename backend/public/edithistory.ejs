<script>
    var ctx = document.getElementById('myChart').getContext('2d');
    var chart = new Chart(ctx, {
        type: 'line',
        data: {
            labels: ["puntos"],
            datasets: [{
                label: '<%=datos.tipoSenal%>',
                fill: false,
                backgroundColor: 'rgb(255, 99, 132)',
                borderColor: 'rgb(255, 99, 132)',
                borderWidth: 2,
                pointRadius: 0.4,
                pointHitRadius: 2,
                lineTension: 0.1,
                data: [<%=datos.DatoSenal%>]
            }]
        },
        options: {
            scales: {
                yAxes: [{
                    stacked: true
                }]
            }
        }
    });
    for (var i=0; i<1000; i++) {
        chart.data.labels.push(i);
    }
</script>
